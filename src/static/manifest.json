{
  "manifest_version": 2,

  "name": "Locatio",
  "description": "Enhance your property search",
  "version": "1.0",

  "permissions": [
    "activeTab",
    "https://google.co.nz/",
    "https://example.com/"
  ],

  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },

  "content_scripts": [
    {
      "matches": [
          "<all_urls>"
        ],
      "js": ["js/app.js"]
    }
  ]
}
